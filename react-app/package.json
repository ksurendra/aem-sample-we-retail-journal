  "scripts": {
    "build-css": "node-sass-chokidar --source-map-embed --include-path ./src --include-path ./node_modules src/ -o src/ --output-style compressed",
    "watch-css": "node-sass-chokidar --source-map-embed --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive --output-style expanded",
    "start-css": "npm-run-all -s build-css watch-css",
    "build-script-prod": "cross-env NODE_ENV=production node scripts/build.js",
    "build-script-dev": "cross-env NODE_ENV=development node scripts/build.js",
    "start-server": "node serverBuild/server.js",
    "start-aem-script": "node scripts/aempack.js",
    "start": "cross-env NODE_ENV=development npm-run-all -p watch-css start-script",
    "build-aem-dev": "npm-run-all -p build-css build-script-dev",
    "build-aem-prod": "npm-run-all -p build-css build-script-prod",
    "build-adobeio": "cross-env ISADOBEIO=true npm-run-all -p build-css build-script-prod ",
    "aempack": "cross-env PROXYHOST=local.we-retail-journal.com NODE_ENV=development npm-run-all -p watch-css start-aem-script",
    "aempack-ssr": "cross-env DEVELOPSSR=true  NODE_ENV=development npm-run-all -p watch-css start-aem-script",
    "aempack-pub": "cross-env PROXYHOST=local.we-retail-journal.com AEMPORT=4503 cross-env NODE_ENV=development npm-run-all -p watch-css start-aem-script",
    "aempack-pub-ssr": "cross-env DEVELOPSSR=true npm-run-all -p aempack-pub"
  },
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "absoluteRuntime": false
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
