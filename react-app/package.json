    "postcss-loader": "3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-safe-parser": "4.0.1",
    "npm-run-all": "^4.1.5",
    "ajv": "^6.4.0",
    "@babel/core": "^7.5.5",
    "@hot-loader/react-dom": "^16.8.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
