{
  "name": "react-app",
  "version": "1.1.1",
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar --source-map-embed --include-path ./src --include-path ./node_modules src/ -o src/ --output-style compressed",
    "watch-css": "node-sass-chokidar --source-map-embed --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive --output-style expanded",
    "linter": "eslint --ext .js,.jsx .",
    "linter:fix": "eslint --ext .js,.jsx . --fix",
    "build": "cross-env NODE_ENV=development webpack",
    "build-server": "cross-env NODE_ENV=development webpack --config webpack.server.config",
    "build:production": "cross-env NODE_ENV=production webpack",
    "test": "npm run build && cross-env NODE_ENV=test karma start --browsers=ChromeHeadless --single-run=true",
    "ci": "npm run test karma-ci.conf.js",
    "test:ci": "scripts/ci/test.sh",
    "test:debug": "npm run build && cross-env NODE_ENV=test karma start --browsers=Chrome_with_debugging",
    "start": "webpack-dev-server --env.dev",
    "start:server": "node --inspect src/server/bootstrap.js",
    "preversion": "npm run test && npm run build:production",
    "version": "git add -A",
    "postversion": "git push && git push --tags",
    "start-aem-script": "node scripts/aempack.js",
    "aemsync": "cross-env PROXYHOST=local.we-retail-journal.com NODE_ENV=development npm-run-all -p watch-css start-aem-script",
    "aemsync-ssr": "cross-env DEVELOPSSR=true  NODE_ENV=development npm-run-all -p watch-css start-aem-script",
    "aemsync-pub-it": "cross-env PROXYHOST=local.we-retail-journal.com AEMPORT=4503 cross-env NODE_ENV=development npm-run-all -p watch-css start-aem-script"
  },
  "proxy": {
    "/content": {
      "target": "http://localhost:4502"
    }
  },
  "dependencies": {
    "@adobe/cq-react-editable-components": "~1.1.0",
    "@adobe/cq-spa-component-mapping": "~1.0.3",
    "@adobe/cq-spa-page-model-manager": "~1.0.7",
    "custom-event-polyfill": "^1.0.6",
    "fbemitter": "^2.1.1",
    "install": "^0.12.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-fast-compare": "^2.0.2",
    "react-open-weather": "^0.3.0",
    "react-router-dom": "^4.3.1",
    "webpack-manifest-plugin": "2.0.4",
    "react-universal-component": "^4.0.0",
    "react-hot-loader": "^4.12.11"
  },
  "peerDependencies": {},
